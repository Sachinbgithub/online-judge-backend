{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "YourMainAppSecretKeyThatMatchesYourMainApp",
    "Issuer": "YourMainAppIssuer",
    "Audience": "YourMainAppAudience",
    "ExpiryMinutes": 60
  },
  "RateLimiting": {
    "GlobalRequestsPerMinute": 1000,
    "CodeExecutionPerMinute": 5,
    "ApiRequestsPerMinute": 2000
  },
  "ContainerPool": {
    "PythonPoolSize": 6,
    "JavaScriptPoolSize": 4,
    "JavaPoolSize": 4,
    "CppPoolSize": 4,
    "CPoolSize": 2,
    "DefaultPoolSize": 4
  }
}
